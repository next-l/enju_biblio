.row
  h1.title
    = t('page.editing', model: t('activerecord.models.resource_import_file'))
  #content_list.col-md-9
    = form_for(@resource_import_file) do |f|
      = f.error_messages
      .field
        = f.label :resource_import_file_name
        br
        = f.text_field :resource_import_file_name
      .actions
        = f.submit
  #submenu.col-md-3
  ul
    li
      = link_to t('page.show'), @resource_import_file
    li
      = link_to t('page.back'), resource_import_files_path
