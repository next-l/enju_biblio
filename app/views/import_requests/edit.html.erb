<div class="row">
  <div class="col-md-9">
    <h1><%= t('page.editing', model: t('activerecord.models.import_request')) -%></h1>

    <%= form_for(@import_request) do |f| -%>
      <%= f.error_messages -%>
      <div class="field">
        <%= f.label :isbn -%><br />
        <%= f.text_field :isbn, class: 'resource_isbn_issn' -%>
      </div>
      <div class="actions">
        <%= f.submit class: 'btn btn-primary' %>
      </div>
    <%- end -%>

  </div>

  <div class="col">
    <ul>
      <li><%= link_to t('page.show'), @import_request -%></li>
      <li><%= link_to t('page.back'), import_requests_path -%></li>
    </ul>
  </div>
</div>
