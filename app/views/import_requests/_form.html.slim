= form_for(@import_request) do |f|
  - if @import_request.errors.any?
    #errorExplanation
      h2
        = pluralize(@import_request.errors.count, "error")
        |  prohibited this import_request from being saved:
      ul
        - @import_request.errors.full_messages.each do |msg|
          li
            = msg
  .field
    = f.label :isbn
    br
    = f.text_field :isbn
  .field
    = f.label :state
    br
    = f.text_field :state
  .field
    = f.label :manifestation_id
    br
    = f.text_field :manifestation_id
  .field
    = f.label :user_id
    br
    = f.text_field :user_id
  .actions
    = f.submit
