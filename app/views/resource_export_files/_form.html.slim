= form_for(@resource_export_file) do |f|
  = f.error_messages
  = f.hidden_field :mode, value: 'export' if @resource_export_file.new_record?
  br
  .form-group
    - if @resource_export_file.new_record?
      = f.submit t('page.export'), class: 'btn btn-default'
    - else
      = f.submit class: 'btn btn-default'
