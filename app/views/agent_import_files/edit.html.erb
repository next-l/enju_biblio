<div class="row">
<div class="col-md-9">
<h1><%= t('page.editing', model: t('activerecord.models.attachment.file')) -%></h1>
<div class="card-body bg-light">

<%= form_for(@attachment.file) do |f| -%>
  <%= f.error_messages -%>

  <div class="actions">
    <%= f.submit class: 'btn btn-primary' %>
  </div>
<%- end -%>

</div>
</div>

<div class="col">
  <ul>
    <li><%= link_to t('page.show'), @attachment.file -%></li>
    <li><%= link_to t('page.back'), attachment.files_path -%></li>
  </ul>
</div>
