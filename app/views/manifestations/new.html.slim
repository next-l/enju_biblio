.row
  #submenu.col-md-3
    ul.list-group
      - if @agent
        li.list-group-item
          = link_to t('page.back'), publisher_manifestations_path(@agent)
      - else
        li.list-group-item
          = link_to t('page.back'), manifestations_path
  #content_list.col-md-9
    h1.title
      = t('page.new', model: t('activerecord.models.manifestation'))
    = render 'manifestations/form'
